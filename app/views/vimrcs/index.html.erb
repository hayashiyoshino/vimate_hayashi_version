<p id="notice"><%= notice %></p>

<h1>Vimrcs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vimrcs.each do |vimrc| %>
      <tr>
        <td><%= link_to 'Show', vimrc %></td>
        <td><%= link_to 'Edit', edit_vimrc_path(vimrc) %></td>
        <td><%= link_to 'Destroy', vimrc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vimrc', new_vimrc_path %>
